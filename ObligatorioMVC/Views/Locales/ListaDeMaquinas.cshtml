@model IEnumerable<ObligatorioMVC.Models.Maquina>

@{
    ViewData["Title"] = "ListaDeMaquinas";
}

<h1>Index</h1>


<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.TipoMaquina)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.VidaUtil)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.TipoMaquina.NombreTipoMaquina)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.VidaUtil)
                </td>
                <td>
                    <a asp-action="Edit" asp-route-id="@item.IdMaquina">Edit</a> |
                    <a asp-action="Details" asp-route-id="@item.IdMaquina">Details</a> |
                    <a asp-action="Delete" asp-route-id="@item.IdMaquina">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
